#include <stdio.h>

int main() {
//t is defined for number of test cases 
    int t;
    scanf("%d", &t);
    while(t--){
        int a,b,x,y;
        scanf("%d%d%d%d", &a, &b, &x, &y);
        if (a * b <= x * y ) {
            printf("yes\n");
        }
        else{
            printf("no\n");
        }
    }
	
    return 0;
}
