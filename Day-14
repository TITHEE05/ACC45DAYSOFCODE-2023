#include <stdio.h>

int main() {
    int X, Y;
    
    printf("Enter the stair number X: ");
    scanf("%d", &X);
    
    printf("Enter the number of steps Chef can climb (Y): ");
    scanf("%d", &Y);
    if(X/Y%0){ 
        printf("minimum number of moves required are: %d",X/Y);
    }else{
        printf("the minimum number of moves required are: %d",X);
    }
    return 0;
}
